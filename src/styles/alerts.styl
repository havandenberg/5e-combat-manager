.alert
  background-color: white
  line-height: 1.75
  margin-bottom: $spacing-small
  padding-left: 2.5rem
  position: relative

  &-error
    color: $error-red

    &:before
      background-color: $error-red
      content:''
      height: 24px
      left: $spacing-micro
      mask: url('data:image/svg+xml;base64, ') no-repeat 50% 50%
      -webkit-mask:  url('data:image/svg+xml;base64, ') no-repeat 50% 50% // @stylint off
      position: absolute
      top: 50%
      transform: translateY(-50%)
      width: 24px

  &-success
    border-color: $success-green
    color: darken($success-green, 90%)

    &:before
      background-color: $success-green
      content:''
      height: 24px
      left: $spacing-micro
      mask: url('data:image/svg+xml;base64, ') no-repeat 50% 50%
      position: absolute
      top: 50%
      transform: translateY(-50%)
      -webkit-mask:  url('data:image/svg+xml;base64, ') no-repeat 50% 50%
      width: 24px

  &-warning
    border-color: $warning-yellow
    color: darken($warning-yellow, 80%)

    &:before
      background-color: $warning-yellow
      content:''
      height: 24px
      left: $spacing-micro
      mask: url('data:image/svg+xml;base64, ') no-repeat 50% 50%
      -webkit-mask:  url('data:image/svg+xml;base64, ') no-repeat 50% 50% // @stylint off
      position: absolute
      top: 50%
      transform: translateY(-50%)
      width: 24px

{
  "name": "5e-combat-manager",
  "version": "0.0.1",
  "description": "D&D 5th Edition Combat Manager",
  "main": "index.js",
  "scripts": {
    "build": "npm run clean && NODE_ENV=production webpack -p --stats --display-error-details --config webpack.dist.config.js",
    "clean": "rm -rf ./dist",
    "start": "webpack-dev-server --config webpack.config.js",
    "lint": "npm run lint:js && npm run lint:stylus",
    "lint:js": "eslint src",
    "lint:stylus": "stylint src/styles",
    "pretest": "npm run lint --silent",
    "test": "mocha --reporter spec src/tests/.setup.js src/tests/**/*.test.js",
    "pretestci": "npm run lint --silent",
    "testci": "MOCHA_FILE=$CIRCLE_TEST_REPORTS/junit/test-result.xml mocha --reporter mocha-junit-reporter src/tests/.setup.js src/tests/**/*.test.js"
  },
  "author": "",
  "dependencies": {
    "classnames": "^2.2.5",
    "firebase": "^3.5.2",
    "immutable": "^3.8.1",
    "lodash": "^4.16.1",
    "moment": "^2.15.2",
    "normalize.css": "^4.2.0",
    "react": "^15.3.1",
    "react-dom": "^15.3.1",
    "react-dropzone": "^3.7.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.8.1",
    "react-router-redux": "^4.0.5",
    "redux": "^3.6.0",
    "redux-thunk": "^2.1.0"
  },
  "devDependencies": {
    "autoprefixer-loader": "^3.2.0",
    "babel-core": "^6.14.0",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.5",
    "babel-preset-airbnb": "^2.0.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "bootprint": "^0.8.5",
    "bootprint-swagger": "^0.13.1",
    "chai": "^3.5.0",
    "clipboard": "^1.5.12",
    "css-loader": "^0.25.0",
    "enzyme": "^2.4.1",
    "eslint": "^3.5.0",
    "eslint-loader": "^1.5.0",
    "eslint-plugin-react": "^6.2.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.22.0",
    "jsdom": "^9.5.0",
    "mocha": "^3.0.2",
    "mocha-junit-reporter": "^1.12.1",
    "open-browser-webpack-plugin": "0.0.2",
    "react-addons-test-utils": "^15.3.1",
    "react-hot-loader": "^3.0.0-beta.3",
    "react-scroll": "^1.4.1",
    "react-test-renderer": "^15.3.1",
    "sinon": "^1.17.5",
    "style-loader": "^0.13.1",
    "stylint": "^1.5.6",
    "stylint-loader": "^1.0.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^2.3.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.15.1"
  }
}

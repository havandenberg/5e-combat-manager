{
  "extends": "./.eslintrc",
  "rules": {
    "block-spacing": 1,
    "brace-style": 1,
    "comma-spacing": 1,
    "default-case": 1,
    "max-len": [1, 140],
    "no-alert": 1,
    "no-console": 1,
    "no-debugger": 1,
    "no-empty": 1,
    "no-multiple-empty-lines": [1, {"max": 2}],
    "no-unused-vars": 1,
    "no-use-before-define": 1,
    "no-var": 1,
    "object-shorthand": 1,
    "react/jsx-closing-bracket-location": [1, 'after-props'],
    "react/jsx-handler-names": 1,
    "react/jsx-key": 1,
    "react/jsx-max-props-per-line": [1, {"maximum": 4}],
    "react/prop-types": 1,
    "react/self-closing-comp": [1, {
      "component": true,
      "html": false
    }],
    "react/sort-prop-types": [1, {"callbacksLast": true}]
  }
}
